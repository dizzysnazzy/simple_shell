#include <stdio.h>

/**
* main - prints program name
* @argc: the number of arguments
* @argv: an array containing the programs's arguments
* Return: 0 always for success
*/

int main(int argc, char *argv[])
{
    int i;
        printf("%d\n",argc);
	    for(i=0;i<argc-1;i++)
	        {
		        printf("%s",argv[i]);
			    }
			        return 0;
				}